#ifndef SRC_KEY_USER_KEY_H_
#define SRC_KEY_USER_KEY_H_


#define KEY  			11         //KEY 连接到 MIO11
#define key0_MIO12     	12     //连接到MIO0
#define key0_EMIO54    	54     //连接到MIO0

typedef struct {
	u32 key_press;
	u32 key_val;
} key_data;
extern key_data key;

extern void key_init(u32 key);


#endif /* SRC_KEY_USER_KEY_H_ */
