void key_init(u32 key){
	GPIO_model_IN(key);
	GPIO_EN(key);
}

void led_interrupt_init(){
		XGpioPs Gpio;

	 //使能来自于Gpio器件的中断
	    XScuGic_Enable(gic_ins_ptr, GpioIntrId);
	 //设置KEY按键的中断类型为下降沿中断
	    XGpioPs_SetIntrTypePin(gpio, KEY, XGPIOPS_IRQ_TYPE_EDGE_FALLING);
	 //使能按键KEY中断
	    XGpioPs_IntrEnablePin(gpio, KEY);

}
